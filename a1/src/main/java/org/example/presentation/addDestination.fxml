<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="450.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.presentation.AddDestinationController">
   <Button layoutX="250.0" layoutY="325.0" mnemonicParsing="false" onAction="#switchToPrimary" text="Back"/>
   <TextField fx:id="destinationNameTextField" layoutX="80.0" layoutY="232.0" prefHeight="31.0" prefWidth="184.0"
              promptText="Destination name"/>
   <Label layoutX="78.0" layoutY="196.0" text="Destination name">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <Button layoutX="367.0" layoutY="171.0" mnemonicParsing="false" onAction="#addDestination" prefHeight="76.0"
           prefWidth="100.0" text="Add Destination" textAlignment="CENTER" wrapText="true"/>
   <Label layoutX="78.0" layoutY="114.0" text="Destination ID">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <TextField fx:id="destinationIdTextField" layoutX="78.0" layoutY="149.0" promptText="Destination ID"/>
</AnchorPane>
